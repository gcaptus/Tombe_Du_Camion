{% extends "base.html.twig" %}

{% block title %}Recherche{% endblock %}

{% block body %}
{% include 'menu.html.twig' %}

<h1>Vos recherches</h1>
{% for vf in product %}            
    <li><strong> Produit {{ vf.designation }} Fabriqué par {{ vf.brand }} : </strong></li>
    <li> - Prix d'achat : {{vf.buyingPrice}} Euros / Prix de vente : {{vf.sellingPrice}} Euros</li>
    <li>- Quantité disponible : {{vf.quantity}}</li>
{% endfor %}




{% endblock %}